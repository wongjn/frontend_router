{#
/**
 * @file
 * Default theme implementation for a router fragment.
 *
 * Available variables:
 * - children: The content for this fragment.
 * - key: Unique identifier for this fragment.
 *
 * @see template_preprocess_router_fragment()
 *
 * @ingroup themeable
 */
#}
<router-content area="{{ key }}">
  {{- children -}}
</router-content>
